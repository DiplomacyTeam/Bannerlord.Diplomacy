<Brushes>
  <Brush Name="Map.WarExhaustionIndicator.Text" Font="Galahad" TextHorizontalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#00FF00FF" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0.15" TextGlowRadius="0" TextBlur="1" FontSize="25" />
      <Style Name="Critical" FontColor="#FF0000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0.15" TextGlowRadius="0" TextBlur="1" FontSize="25" />
    </Styles>
  </Brush>

  <Brush Name="Map.WarExhaustionReport.Text" Font="FiraSansExtraCondensed-Regular" TextHorizontalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#E1E1E1FF" TextOutlineAmount="0" TextGlowRadius="0.0" TextBlur="0" TextShadowOffset="0" FontSize="24" />
      <Style Name="Critical" FontColor="#FF0000FF" TextOutlineAmount="0" TextGlowRadius="0.0" TextBlur="0" TextShadowOffset="0" FontSize="24" />
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Faction.HelpButton" TextHorizontalAlignment="Center">
    <Styles>
      <Style Name="Default" FontSize="22" FontColor="#BC9142FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" TextShadowOffset="1" TextShadowAngle="60" />
      <Style Name="Hovered" FontColor="#BC9142FF" TextColorFactor="1.3" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" TextShadowOffset="1" TextShadowAngle="60" />
      <Style Name="Disabled" FontColor="#BC9142FF" TextColorFactor="0.4" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" TextShadowOffset="1" TextShadowAngle="60" />
      <Style Name="Pressed" FontColor="#BC9142FF" TextColorFactor="0.8" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" TextShadowOffset="1" TextShadowAngle="60" />
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Icon.WarIcon">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\MapOverlay\Encounter\WarIcon" />
      <BrushLayer Name="Pre161" Sprite="MapOverlay\Encounter\WarIcon" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="1" />
        <BrushLayer Name="Pre161" AlphaFactor="1" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Icon.Retreat">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\GameMenu\retreat_icon" />
      <BrushLayer Name="Pre161" Sprite="GameMenu\retreat_icon" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="1" />
        <BrushLayer Name="Pre161" AlphaFactor="1" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Icon.Gold">
    <Layers>
      <BrushLayer Name="Default" Sprite="General\Icons\Coin@2x" />
      <BrushLayer Name="Pre161" Sprite="Icons\Coin@2x" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="1" />
        <BrushLayer Name="Pre161" AlphaFactor="1" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Icon.Circle">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\Nameplates\hover_ring" Color="#BC9142FF"/>
      <BrushLayer Name="Pre161" Sprite="Nameplates\hover_ring" Color="#BC9142FF"/>
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="1" />
        <BrushLayer Name="Pre161" AlphaFactor="1" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.3" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.8" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.Frame1Canvas.Alt">
    <Layers>
      <BrushLayer Name="Default" Sprite="Frame1_canvas" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="3" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Diplomacy.FlatBanner.Normal">
    <Layers>
      <BrushLayer Name="Default" Sprite="StdAssets\banner_flat" OverlayXOffset="40" OverlayYOffset="35" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.3" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.8" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
      </Style>
    </Styles>
  </Brush>

</Brushes>